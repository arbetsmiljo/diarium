{
  "bin": {
    "diarium": "./dist/bin.js"
  },
  "dependencies": {
    "better-sqlite3": "^11.8.1",
    "commander": "^13.1.0",
    "cross-fetch": "^4.1.0",
    "date-fns": "^4.1.0",
    "jsdom": "^26.0.0",
    "kysely": "^0.27.5",
    "lodash": "^4.17.21",
    "ora-classic": "^5.4.2",
    "zod": "^3.24.1"
  },
  "description": "SQLite & web scraper tooling for work environment data",
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.12",
    "@types/jest": "^29.5.14",
    "@types/jsdom": "^21.1.7",
    "@types/lodash": "^4.17.14",
    "@types/node": "^20",
    "jest": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "ts-jest": "^29.2.5",
    "tsx": "^4.19.2",
    "typedoc": "^0.27.6",
    "typescript": "^5.7.3"
  },
  "exports": {
    "node": "./dist/index.js",
    "types": "./dist/index.d.ts"
  },
  "files": [
    "dist/*.js",
    "dist/*.cjs",
    "dist/*.d.ts"
  ],
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+.tsx?$": [
        "ts-jest",
        {}
      ]
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!node-fetch)"
    ]
  },
  "name": "@arbetsmiljo/diarium",
  "pnpm": {
    "onlyBuiltDependencies": [
      "better-sqlite3"
    ]
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -f *.js; rm -f *.d.ts; rm -rf dist",
    "diarium": "tsx ./src/bin.ts",
    "test": "jest"
  },
  "typedocOptions": {
    "entryPoints": [
      "./src/case.ts",
      "./src/company.ts",
      "./src/county.ts",
      "./src/database.ts",
      "./src/document.ts",
      "./src/ingestion.ts",
      "./src/municipality.ts",
      "./src/pagination.ts",
      "./src/time.ts",
      "./src/workplace.ts"
    ]
  },
  "type": "module",
  "types": "./dist/index.d.ts",
  "version": "0.0.24"
}
