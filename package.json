{
  "bin": {
    "diarium": "./dist/bin.js"
  },
  "dependencies": {
    "commander": "^13.1.0",
    "cross-fetch": "^4.1.0",
    "jsdom": "^26.0.0",
    "lodash": "^4.17.21",
    "ora-classic": "^5.4.2",
    "sqlite3": "^5.1.7",
    "zod": "^3.24.1"
  },
  "description": "SQLite & web scraper tooling for work environment data",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/jsdom": "^21.1.7",
    "@types/lodash": "^4.17.14",
    "@types/node": "^20",
    "jest": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "ts-jest": "^29.2.5",
    "tsx": "^4.19.2",
    "typedoc": "^0.27.6",
    "typescript": "^5.7.3"
  },
  "exports": {
    "./*": "./dist/*"
  },
  "files": [
    "dist/*.js",
    "dist/*.cjs",
    "dist/*.d.ts"
  ],
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+.tsx?$": [
        "ts-jest",
        {}
      ]
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!node-fetch)"
    ]
  },
  "name": "@arbetsmiljo/diarium",
  "scripts": {
    "build": "tsc",
    "clean": "rm -f *.js; rm -f *.d.ts; rm -rf dist",
    "diarium": "tsx ./src/bin.ts",
    "test": "jest"
  },
  "typedocOptions": {
    "entryPoints": [
      "./src/database.ts",
      "./src/document.ts",
      "./src/ingestion.ts",
      "./src/pagination.ts"
    ]
  },
  "types": "./dist/index.d.ts",
  "version": "0.0.15"
}
